
struct A {int x}
struct B {int x; bool y}

bool equal(A? first, A? second) {
  return first == second;
}

A? to_a(B? b) {
  if? (A a = b) {
    return a;
  }
  return A null;
}

int program(int argc, string[] argv) {
  var a1 = new A { x = 10};
  var a2 = A null;
  a2 = a1;

  if (equal(a1, a2)) {
    return 42;
  }
  return 15;
}
